{
	"admin": {
		"disabled": true
	},
	"apps": {
		"http": {
			"servers": {
				"srv0": {
					"listen": [
						":445"
					],
					"routes": [{
						"handle": [{
							"handler": "subroute",
							"routes": [{
									"handle": [{
										"auth_user_deprecated": "User",
										"auth_pass_deprecated": "71939753-4f72-427a-bd80-a3c761e19f10",
										"handler": "forward_proxy",
										"hide_ip": true,
										"hide_via": true
									}]
								},
								{
									"match": [{
										"host": [
											"www.xiaoxuesheng.top"
										]
									}],
									"handle": [{
										"handler": "file_server",
										"root": "/var/www/html",
										"index_names": [
											"index.html"
										]
									}],
									"terminal": true
								}
							]
						}]
					}],
					"tls_connection_policies": [{
						"match": {
							"sni": [
								"www.xiaoxuesheng.top"
							]
						}
					}],
					"automatic_https": {
						"disable": true
					}
				}
			}
		},
		"tls": {
			"certificates": {
				"load_files": [{
					"certificate": "/etc/letsencrypt/live/www.xiaoxuesheng.top/fullchain.pem",
					"key": "/etc/letsencrypt/live/www.xiaoxuesheng.top/privkey.pem"
				}]
			}
		}
	}
}
